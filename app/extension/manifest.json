{
  "name": "Don't Dox Me",
  "description": "A chrome extension, which prevents you getting",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "scripts/service_worker/background.js"
  },
  "content_scripts": [
    {
      "js": ["scripts/script_loader.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["scripts/inject/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
