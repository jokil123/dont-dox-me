{
  "name": "Don't Dox Me",
  "description": "A chrome extension, which prevents you getting",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "service_workers/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["service_workers/script_loader.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["service_workers/index.js", "service_workers/toImport.js"]
    }
  ]
}
